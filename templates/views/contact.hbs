<div id="Contact">
	<div class="container">
		<div class="heading">Contact Us</div>
		<div class="images">
			{{#each images}}
			<img src="{{image.url}}" alt="" height="100%;">
			{{/each}}
		</div>
		<div class="form">
			{{#if enquirySubmitted}}
			<h3>Thanks for getting in touch.</h3>
			{{else}}
			<form method="post">
				<div>
					<input type="hidden" name="action" value="contact">
					<div class="form-group {{#if validationErrors.name}}has-error{{/if}}">
						<input type="text" name="name.full" class="form-control" value="{{formData.[name.full]}}" placeholder="Name.">
					</div>
					<div class="form-group {{#if  validationErrors.email}}has-error{{/if}}">
						<input type="email" name="email" class="form-control" value="{{formData.email}}" placeholder="Email.">
					</div>
				</div>
				<div>
					<div class="form-group {{#if validationErrors.message}}has-error{{/if}} ">
						<textarea name="message" placeholder="Message." class="form-control">{{formData.message}}</textarea>
					</div>
					<div class="form-group">
						<button type="submit">Send</button>
					</div>
				</div>

			</form>
			{{/if}}
		</div>
	</div>
</div>