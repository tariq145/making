<div id="Player">

   <div class="video">
      <iframe src="https://www.youtube.com/embed/NWL3cvOcrkk" frameborder="0" allowfullscreen></iframe>
   </div>

   <div class="description">
      <div class="title">
         {{data.main.title}}
      </div>
      <div class="main-description">
         {{data.main.description}}
      </div>
   </div>

   <div class="player-feed">
         {{# each data.videos}}
            <a href="/player/{{slug}}">
               <div class="feed-item">
                  <div class="feed-item" style="
                  background: url('{{image.secure_url}}') no-repeat center center;
                  -webkit-background-size: cover;
                  -moz-background-size: cover;
                  -o-background-size: cover;
                  background-size: cover;
                  ">
            <span class="name">{{title}}</span>
         </div>
               </div>
            </a>
         {{/each}}
   </div>
</div>