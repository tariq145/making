<div id="Player">

   <div class="video">
      <iframe src="https://www.youtube.com/embed/NWL3cvOcrkk" frameborder="0" allowfullscreen></iframe>
   </div>

   <div class="description">
      <div class="title">
         {{data.main.title}}
      </div>
      <div class="main-description">
         {{data.main.description}}
      </div>
   </div>

   <div class="player-feed">
      this is a player feed
      <ul>
         {{# each data.videos}}
         <li>
            <a href="/player/{{slug}}">{{title}}</a>
         </li>
         {{/each}}
      </ul>
   </div>
</div>